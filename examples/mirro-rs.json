{
  "general": {
    "outfile": "~/example/generated-mirrors",
    "export": 50,
    "view": "alphabetical",
    "sort": "score",
    "cache-ttl": 24,
    "url": "https://archlinux.org/mirrors/status/json/",
    "rate-speed": true,
    "timeout": 5
  },
  "filters": {
    "countries": [],
    "age": 24,
    "ipv6": true,
    "ipv4": true,
    "isos": true,
    "protocols": [
      "https",
      "http",
      "rsync"
    ],
    "completion-percent": 100
  }
}
